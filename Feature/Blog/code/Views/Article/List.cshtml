@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation

<h1> This is the Article List</h1>

@model Sitecore.Feature.Blog.ViewModels.ArticleListViewModel

@foreach (var articleItem in Model.ArticleList)
{
    <a href="@articleItem.Url">@articleItem.ArticleModel.Title</a>
    <div>
        @articleItem.ArticleModel.Summary
    </div>
}


